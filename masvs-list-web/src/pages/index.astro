---
export const prerender = false
import Layout from '../layouts/Layout.astro';
const token = Astro.cookies.get("token");
const user = Astro.cookies.get("user");
if (!token || !user) {
  return Astro.redirect("/login");
}else{
  return Astro.redirect("/dashboard");
}


